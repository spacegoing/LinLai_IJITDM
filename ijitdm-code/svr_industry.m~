function [ output_args ] = svr_industry(industry_name)
%SVR_INDUSTRY Calculate one industry's stats

%% Data processing

train_num = 309;

% read xlsx data
raw_data = xlsread(['28_Industs/' industry_name]);

%%%%%%%%%%%%%%%%%% Train Data %%%%%%%%%%%%%%%%%%%
raw_sent_X = raw_data(1:train_num,2:11)';
raw_no_sent_X = raw_data(1:train_num,3:11)';
raw_Y = raw_data(1:train_num,12)';

% normalization
% the result is m by n (number of instances) matrix
[sent_X,sent_input_attrs] = mapminmax(raw_sent_X);
[no_sent_X,no_sent_input_attrs] = mapminmax(raw_no_sent_X);
[Y,output_attrs] = mapminmax(raw_Y);

%%%%%%%%%%%%%%%%%% Test Data %%%%%%%%%%%%%%%%%%%
% read xlsx data
raw_sent_X_t = raw_data_t(:,2:11)';
raw_no_sent_X_t = raw_data_t(:,3:11)';
raw_Y_t = raw_data_t(:,12)';

% normalization
% the result is m by n (number of instances) matrix
[sent_X_t,sent_input_attrs] = mapminmax(raw_sent_X_t);
[no_sent_X_t,no_sent_input_attrs] = mapminmax(raw_no_sent_X_t);
[Y,output_attrs] = mapminmax(raw_Y);



end

